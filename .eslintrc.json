{
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {
        "module": true,
        "result": true,
        "round": true,
        "google": true
    },
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-undef": [
            "error"
        ],
        "strict": [
            "error",
            "global"
        ],
        "no-console": [
            "error"
        ],
        "comma-dangle": [
            "error"
        ],
        "max-len": [
            "error",
            80,
            2,
            {"ignoreUrls": true}
        ],
        "no-trailing-spaces": [
            "error"
        ],
        "curly": [
            "error"
        ],
        "newline-before-return": [
            "error"
        ],
        "eqeqeq": [
            "error"
        ]
    }
}
